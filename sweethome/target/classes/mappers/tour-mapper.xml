<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="tourMapper">
	<resultMap type="tour" id="tourResultSet">
		<result column="TOUR_NO" property="tourNo" />
		<result column="TOUR_NO" property="houseNo" />
		<result column="TOUR_NO" property="houseName" />
		<result column="TOUR_NO" property="roomName" />
		<result column="TOUR_NO" property="userName" />
		<result column="TOUR_NO" property="gender" />
		<result column="TOUR_NO" property="age" />
		<result column="TOUR_NO" property="phone" />
		<result column="TOUR_NO" property="email" />
		<result column="TOUR_NO" property="askContent" />
	</resultMap>
	
	<select id="userTourListCount" resultType="_int">
	  	SELECT 
	  			COUNT(1)
	  	  FROM 
	  			TOUR
	  	 WHERE
	  	 		USER_NO = #{userNo}
	</select>
  
</mapper>
