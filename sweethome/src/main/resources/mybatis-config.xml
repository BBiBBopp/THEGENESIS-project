<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.thegenesis.sweethome.member.model.vo.Member" alias="member" />
		<typeAlias type="com.thegenesis.sweethome.chat.model.vo.Chat" alias="chat" />
		<typeAlias type="com.thegenesis.sweethome.ask.model.vo.Ask" alias="ask" />
		<typeAlias type="com.thegenesis.sweethome.ask.model.vo.AskFile" alias="askFile" />
		<typeAlias type="com.thegenesis.sweethome.member.model.vo.Cert" alias="cert" />
		<typeAlias type="com.thegenesis.sweethome.community.model.vo.Community" alias="community" />
		<typeAlias type="com.thegenesis.sweethome.community.model.vo.Reply" alias="reply" />
		<typeAlias type="com.thegenesis.sweethome.community.model.vo.CommunityFile" alias="communityFile"/>
		<typeAlias type="com.thegenesis.sweethome.community.model.vo.Category" alias="category"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="/mappers/member-mapper.xml" />
		<mapper resource="/mappers/ask-mapper.xml"	/>
		<mapper resource="/mappers/community-mapper.xml"/>
	</mappers>
</configuration>
